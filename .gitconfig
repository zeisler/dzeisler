[user]
	name = Dustin Zeisler
	email = dustin@zeisler.net
[branch]
	autosetuprebase = always
[credential]
	helper = osxkeychain
[core]
	excludesfile = /Users/dzeisler/.gitignore_global
	editor = subl -n -w
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[push]
	default = simple
[rerere]
	enabled = true
